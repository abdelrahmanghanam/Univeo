<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_test"
    app:startDestination="@id/testInitialFragment">

    <fragment
        android:id="@+id/testInitialFragment"
        android:name="ghanam.com.univeo.test.testInitialFragment"
        android:label="fragment_test_initial"
        tools:layout="@layout/fragment_test_initial" >
        <action
            android:id="@+id/action_testInitialFragment_to_testQuestionsFragment"
            app:destination="@id/testQuestionsFragment" />
    </fragment>
    <fragment
        android:id="@+id/testQuestionsFragment"
        android:name="ghanam.com.univeo.test.testQuestionsFragment"
        android:label="fragment_test_questions"
        tools:layout="@layout/fragment_test_questions" >
        <action
            android:id="@+id/action_testQuestionsFragment_to_testResultFragment"
            app:destination="@id/testResultFragment" />
    </fragment>
    <fragment
        android:id="@+id/testResultFragment"
        android:name="ghanam.com.univeo.test.testResultFragment"
        android:label="fragment_test_result"
        tools:layout="@layout/fragment_test_result" />
</navigation>